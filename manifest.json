{
  "manifest_version": 3,
  "name": "Phishing Detector",
  "version": "1.0",
  "description": "A Chrome extension to detect phishing websites.",
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "webRequest",
    "webRequestBlocking"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "host_permissions": ["<all_urls>"]
}
